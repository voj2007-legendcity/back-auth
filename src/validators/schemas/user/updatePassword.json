{
  "$id": "updateUserPassword.json#",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "_id": { "type": "string" },
    "password": {
      "type": "string",
      "minLength": 6
    }
  },
  "required": [
    "_id",
    "password"
  ],
  "additionalProperties": false,
  "errorMessage": {
    "properties": {
      "_id": "_ID_IS_INVALID",
      "name": "PASSWORD_IS_ICORRECT"
    }
  }
}
